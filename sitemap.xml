---
layout: none
search_exclude: true
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages -%}
  {% if page.url == "/index" or page.search_exclude != true and page.title %}
  <url>
    <loc>{{ site.url }}{{ page.url | remove: "index" }}</loc>
  </url>
  {% endif -%}
  {% endfor -%}
</urlset>
